  <xs:element name="d2LogicalModel" type="D2LogicalModel:D2LogicalModel">
    <xs:unique name="_ParkingAreaConstraint">
      <xs:selector xpath=".//ParkingArea" />
      <xs:field xpath="@id" />
      <xs:field xpath="@version" />
    </xs:unique>
    <xs:unique name="_ParkingFacilityConstraint">
      <xs:selector xpath=".//ParkingFacility" />
      <xs:field xpath="@id" />
      <xs:field xpath="@version" />
    </xs:unique>
  </xs:element>
  <xs:complexType name="D2LogicalModel">
    <xs:annotation>
      <xs:documentation>The DATEX II logical model comprising exchange, content payload and management sub-models.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="exchange" type="D2LogicalModel:Exchange" />
      <xs:element name="payloadPublication" type="D2LogicalModel:PayloadPublication" minOccurs="0" />
      <xs:element name="d2LogicalModelExtension" type="D2LogicalModel:_ExtensionType" minOccurs="0" />
    </xs:sequence>
    <xs:attribute name="modelBaseVersion" use="required" fixed="2" />
    <xs:attribute name="extensionName" use="optional" default="MDM" />
    <xs:attribute name="extensionVersion" use="optional" default="00-01-03" />
  </xs:complexType>
  <xs:simpleType name="DateTime">
    <xs:annotation>
      <xs:documentation>A combination of integer-valued year, month, day, hour, minute properties, a decimal-valued second property and a time zone property from which it is possible to determine the local time, the equivalent UTC time and the time zone offset from UTC.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:dateTime" />
  </xs:simpleType>
  <xs:complexType name="DayType">
    <xs:annotation>
      <xs:documentation>Specification of the type of day.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="generalDayType" type="D2LogicalModel:GeneralDayTypeEnum" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Specification of the general day type.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element name="publicHoliday" type="D2LogicalModel:PublicHoliday" minOccurs="0" maxOccurs="unbounded" />
      <xs:element name="dayTypeExtension" type="D2LogicalModel:_ExtensionType" minOccurs="0" />
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="DescriptionOfAssignedParkingSpaces">
    <xs:annotation>
      <xs:documentation>Description of the type of assigned parking spaces.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element name="parkingDuration" type="D2LogicalModel:ParkingDurationEnum" minOccurs="0" maxOccurs="1">
        <xs:annotation>
          <xs:documentation>Parking duration for which the parking spaces are assigned.</xs:documentation>
